package compilerproject2;

import java.util.*;

/**
 *
 * @author Michael
 */
public class Program {

    private ArrayList<Decl> declList;

    public Program(ArrayList<Decl> a) {
        declList = a;
    }

    public void print() {
        String w = "    ";
        System.out.println("program:");
        while (!declList.isEmpty()) {
            declList.remove(0).print(w);
        }
    }
}
