		args = a;
		simpleExpressionP = sep;
	}
	
	public ExpressionP (SimpleExpressionP sep){
		simpleExpressionP = sep;
	}

	public static ExpressionP parseExpressionP(Parser parser) {
		// TODO Auto-generated method stub
		return null;
	}

}
